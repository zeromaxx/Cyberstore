@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="chart-area">
    <div class="chart-container">
        <div>
            <figure class="highcharts-figure">
                <div id="container-1"></div>
            </figure>
        </div>
        <div>
            <figure class="highcharts-figure">
                <div id="container-2"></div>
            </figure>
        </div>
    </div>

    <div class="chart-container-1">
